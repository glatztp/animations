{
  "name": "@gltz-packages/animation-hub",
  "version": "1.0.1",
  "description": "Biblioteca profissional de animacoes corporativas para React com GSAP e TypeScript",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "gsap": "^3.13.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsup",
    "demo:dev": "vite",
    "demo:build": "vite build",
    "demo:preview": "vite preview",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy:patch": "npm version patch && npm publish",
    "deploy:minor": "npm version minor && npm publish",
    "deploy:major": "npm version major && npm publish",
    "clean": "rimraf dist",
    "prepare": "npm run build"
  },
  "keywords": [
    "react",
    "typescript",
    "animations",
    "gsap",
    "corporate",
    "ui",
    "components",
    "motion",
    "transitions",
    "scroll-animations",
    "performance",
    "accessibility",
    "library",
    "gltz-packages"
  ],
  "author": {
    "name": "Gabriel Latz",
    "email": "gabriel@gltz.dev",
    "url": "https://github.com/glatztp"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/glatztp/animation-hub.git"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "tsup": "^8.5.0",
    "typescript": "~5.7.2",
    "vite": "^7.0.4",
    "eslint": "^9.17.0"
  },
  "dependencies": {
    "gsap": "^3.13.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
