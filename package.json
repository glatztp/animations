{
  "name": "@glatztp/animation-hub",
  "version": "1.0.0",
  "description": "Biblioteca profissional de animações corporativas para React com GSAP e TypeScript",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "gsap": "^3.13.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsup",
    "demo:dev": "vite",
    "demo:build": "vite build",
    "demo:preview": "vite preview",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy:patch": "npm version patch && npm publish",
    "deploy:minor": "npm version minor && npm publish",
    "deploy:major": "npm version major && npm publish",
    "clean": "rimraf dist",
    "prepare": "npm run build"
  },
  "keywords": [
    "react",
    "typescript",
    "animations",
    "gsap",
